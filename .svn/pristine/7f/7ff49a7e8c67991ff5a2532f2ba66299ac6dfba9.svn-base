<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.inogard.enio.agent.mappers.rfq.RfqDegreeMapper">
  <sql id="sqlSelect">
    SELECT rd.rfq_no, 
           rd.quot_rev, 
           rd.bid_start_dt, 
           rd.bid_expire_dt, 
           rd.bid_open_dt, 
           rd.rebid_rmrk 
    FROM   agt_rfqdegree rd 
  </sql>

  <select id="findAllByRfqNo" resultType="rfqDegree">
    <include refid="sqlSelect" />
    <![CDATA[
      WHERE  rd.rfq_no = #{rfqNo} 
    ]]>
  </select>

  <insert id="add" parameterType="rfqDegree">
  <![CDATA[
    INSERT INTO agt_rfqdegree (
      rfq_no, 
      quot_rev, 
      bid_start_dt, 
      bid_expire_dt, 
      bid_open_dt, 
      rebid_rmrk
    ) VALUES (
      #{rfqNo}, 
      #{quotRev}, 
      To_char(#{bidStartDt}, 'YYYYMMDDHH24MISS'), 
      To_char(#{bidExpireDt}, 'YYYYMMDDHH24MISS'), 
      To_char(#{bidOpenDt}, 'YYYYMMDDHH24MISS'), 
      #{rebidRmrk}
    )      
  ]]>
  </insert>
</mapper>