server:
  port: 8081
  
spring:
  mvc:
    view:
      prefix: /WEB-INF/views/
      suffix: .jsp
      
mybatis:
  typeAliasesPackage: kr.co.inogard.enio.agent.domain
  mapperLocations: classpath*:**/mappers/**/*.xml
  configuration:
    lazyLoadingEnabled: true
    mapUnderscoreToCamelCase: true
    jdbcTypeForNull: varchar
    
endpoints:
  jmx:
    domain: enio-agent
    unique-names: true
    
enio:
  welcome:
    message: Welcome Enio Agent!
  api:
    version: v1
    context-path: /api
    uri-path:
      item: /items
      dept: /depts
      user: /users
      pr: /pr
      
multipart:
  max-file-size: 30MB
  max-request-size: 300MB