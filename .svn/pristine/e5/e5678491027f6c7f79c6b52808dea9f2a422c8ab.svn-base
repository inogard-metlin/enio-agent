<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.inogard.enio.agent.mappers.agent.AgentMapper">

  <select id="findOne" parameterType="string" resultType="kr.co.inogard.enio.agent.domain.agent.Agent">
  <![CDATA[
	  SELECT agt.agt_cd, 
           agt.licence_key, 
           agt.admin_id, 
           agt.admin_pwd, 
           agt.api_cd,
           agt.api_licence_key, 
           agt.api_conn_url, 
           dbo.getStringToDateTime(agt.reg_dt) AS reg_dt
    FROM   agt_agent agt 
  ]]>
  </select>
</mapper>
